{
  "name": "webpack-starter-theme",
  "description": "Silverstripe theme using Bootstrap 5",
  "version": "1.0.0",
  "private": true,
  "repository": "dynamic/webpack-starter-theme",
  "license": "MIT",
  "stackblitz": {
    "startCommand": "npm start"
  },
  "scripts": {
    "build": "webpack build --mode=production",
    "test": "npm run build",
    "dev": "webpack --watch --stats-error-details",
    "lint:styles": "stylelint src/scss/**/*.scss",
    "lint:styles:fix": "stylelint src/scss/**/*.scss --fix",
    "lint:scripts": "eslint src/javascript/**/*.js",
    "lint:scripts:fix": "eslint src/javascript/**/*.js --fix"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.0"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.12.1",
    "autoprefixer": "^10.4.14",
    "css-loader": "^6.8.1",
    "eslint": "^8.4.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-webpack-plugin": "^3.1.1",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss-loader": "^7.3.3",
    "sass": "^1.64.1",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "stylelint": "^14.1.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-webpack-plugin": "^3.1.0",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  }
}
